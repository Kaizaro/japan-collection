{
  "name": "museumapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "start-reset-cache": "react-native start --reset-cache",
    "react-native-install": "yarn install && cd ios && pod install && cd ..",
    "react-native-reinstall": "rm -rf node_modules && yarn install && cd ios && pod install --repo-update && cd ..",
    "react-native-full-clean": "watchman watch-del-all && rm -rf node_modules && rm -rf $TMPDIR/haste-map-react-native-packager-* && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && xcodebuild -project ios/mobikey.xcodeproj -configuration Release clean && xcodebuild -project ios/mobikey.xcodeproj -configuration Debug clean && rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf ios/build && rm -rf android/build && cd android && ./gradlew clean && cd ../ios && pod deintegrate && cd .. && yarn install && cd ios && pod install",
    "pod-install": "cd ios && pod install && cd ..",
    "apk-release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "bundle-release": "cd android && ./gradlew clean && ./gradlew assembleRelease && ./gradlew bundleRelease && cd ..",
    "android-clean": "cd android && rm -rf build && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-audiowaveform": "^2.0.5",
    "react-native-gesture-handler": "^2.4.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-photo-view": "^1.5.2",
    "react-native-reanimated": "^2.7.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-track-player": "^2.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.17",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.181",
    "@types/react": "^17.0.34",
    "@types/react-native": "^0.66.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-module-resolver": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "flipper-plugin-react-native-performance": "^0.6.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "metro-react-native-babel-transformer": "^0.64.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "17.0.2",
    "typescript": "4.6.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
